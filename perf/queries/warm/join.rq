PREFIX ear: <https://example.org/ear#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX prov: <http://www.w3.org/ns/prov#>
# @group: join
SELECT ?paragraph ?source
WHERE {
  ?reg ear:hasSection ?section .
  ?section ear:hasParagraph ?paragraph .
  ?paragraph dct:source ?source ;
             prov:wasDerivedFrom ?entity .
}
ORDER BY ?paragraph
LIMIT 10
